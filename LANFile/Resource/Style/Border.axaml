<Styles xmlns="https://github.com/avaloniaui"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">


    <Style Selector="Border.VerticalM1">
        <Setter Property="Background" Value="{StaticResource Border.DarkBase}" />
        <Setter Property="Margin" Value="4" />
        <Setter Property="Width" Value="50" />
        <Setter Property="HorizontalAlignment" Value="Center" />

    </Style>
    <Style Selector="Border.BorderM1">
        <Setter Property="CornerRadius" Value="4" />
        <Setter Property="Background" Value="{StaticResource Border.DarkBase}" />
        <Setter Property="BorderThickness" Value="1" />
        <Setter Property="BorderBrush" Value="{StaticResource Border.DarkBaseBorderBrush}" />
    </Style>
</Styles>