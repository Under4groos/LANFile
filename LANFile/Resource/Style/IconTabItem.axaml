        xmlns:controls="clr-namespace:LANFile.Views.Controls">
    <Design.PreviewWith>
        <Border >
            <!-- Add Controls for Previewer Here -->
        </Border>
    </Design.PreviewWith>

    <Style Selector="controls|IconTabItem.Buttom">
