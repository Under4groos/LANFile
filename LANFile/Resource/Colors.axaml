<ResourceDictionary xmlns="https://github.com/avaloniaui"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                    xmlns:sys="clr-namespace:System;assembly=mscorlib"
                    >
    <SolidColorBrush x:Key="Window.Base" Color="#101010"/>
    
    
    <SolidColorBrush x:Key="ButtonBackground" Color="#141414" />
    <SolidColorBrush x:Key="ButtonForeground" Color="#dfdfd6" />
    <SolidColorBrush x:Key="ButtonBorderBrush" Color="#212121" />
    <SolidColorBrush x:Key="ButtonBackgroundPressed" Color="#141414" />
    

    
    
    <SolidColorBrush x:Key="Border.DarkBase" Color="#202020" />
    <SolidColorBrush x:Key="Border.DarkBaseBorderBrush" Color="White" Opacity="0.05" />
    
    <SolidColorBrush x:Key="TabItem.Base"/>
    <SolidColorBrush x:Key="TabItem.Hover"/>
    <SolidColorBrush x:Key="TabItem.Select"/>
</ResourceDictionary>